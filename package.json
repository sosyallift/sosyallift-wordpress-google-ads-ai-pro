{
    "name": "sosyallift-ai-pro",
    "version": "2.0.0",
    "description": "WordPress plugin for AI-powered SEO and Ads intelligence",
    "main": "assets/js/admin/app.js",
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "build": "npm run production",
        "lint": "eslint assets/js/**/*.js",
        "lint:fix": "eslint assets/js/**/*.js --fix",
        "styles": "sass assets/scss:assets/css --style=compressed",
        "styles:watch": "sass --watch assets/scss:assets/css"
    },
    "keywords": [
        "wordpress",
        "seo",
        "ads",
        "ai",
        "analytics"
    ],
    "author": "Sosyallift",
    "license": "GPL-3.0",
    "devDependencies": {
        "laravel-mix": "^6.0.0",
        "resolve-url-loader": "^5.0.0",
        "sass": "^1.32.0",
        "sass-loader": "^12.0.0",
        "vue-template-compiler": "^2.6.14",
        "webpack": "^5.0.0",
        "eslint": "^8.0.0",
        "eslint-config-wordpress": "^4.0.0",
        "axios": "^0.21.0",
        "chart.js": "^3.0.0",
        "apexcharts": "^3.35.0",
        "datatables.net": "^1.13.0",
        "datatables.net-dt": "^1.13.0",
        "select2": "^4.1.0"
    }
}